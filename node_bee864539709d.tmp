let cpf = [1,2,3,4,5,6,7,8,9,0,9]

function validCpf(cpf = []){
    let position = 0
    let count = 10 
    do{
        cpf.push(cpf[position] * count--)

    }while(position < 9)
    return cpf
}
cpf = validCpf()
